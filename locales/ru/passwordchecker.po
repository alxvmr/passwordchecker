#: passwordchecker/passwordchecker-ui.c:385
msgid "Connection settings have been successfully changed"
msgstr "Настройки соединения успешно изменены"

#: passwordchecker/passwordchecker-ui.c:394
msgid "Unable to change connection settings"
msgstr "Не удалось изменить параметры соединения"

#: passwordchecker/passwordchecker-ui.c:416
msgid "Time units can't be empty"
msgstr "Поле не может быть пустым"

#: passwordchecker/passwordchecker-ui.c:421
msgid "Time units can't be negative"
msgstr "Значение не может быть отрицательным"

#: passwordchecker/passwordchecker-ui.c:426
msgid "The record contains invalid characters"
msgstr "Значение содержит недопустимые символы"

#: passwordchecker/passwordchecker-ui.c:496
msgid "Application settings have been successfully changed"
msgstr "Настройки приложения успешно изменены"

#: passwordchecker/passwordchecker-ui.c:501
msgid "Unable to change application settings"
msgstr "Не удалось изменить настройки приложения"

#: passwordchecker/passwordchecker-ui.c:576
#: passwordchecker/passwordchecker-ui.c:644
msgid "Application"
msgstr "Приложение"

#: passwordchecker/passwordchecker-ui.c:592
#: passwordchecker/passwordchecker-ui.c:676
msgid "Connection"
msgstr "Соединение"

#: passwordchecker/passwordchecker-ui.c:635
msgid "Notification start time"
msgstr "Время начала напоминаний"

#: passwordchecker/passwordchecker-ui.c:636
msgid "How much time to warn the user about password expiration"
msgstr "За какое время нужно предупредить пользователя об истечении пароля"

#: passwordchecker/passwordchecker-ui.c:639
msgid "Frequency of warnings"
msgstr "Частота напоминаний"

#: passwordchecker/passwordchecker-ui.c:640
msgid "Sets the frequency of password change warning output"
msgstr "Частота вывода напоминаний"

#: passwordchecker/passwordchecker-ui.c:648
msgid "Apply application settings"
msgstr "Изменить параметры приложения"

#: passwordchecker/passwordchecker-ui.c:669
msgid "LDAP server address"
msgstr "Адрес LDAP сервера (url)"

#: passwordchecker/passwordchecker-ui.c:670
msgid "Specifies the LDAP server address (e.g. ldap://dc1.domain.test.ru)"
msgstr ""
"Задает адрес LDAP сервера.\n"
"Например: ldap://dc1.domain.test.ru"

#: passwordchecker/passwordchecker-ui.c:673
msgid "Search root"
msgstr "Корень поиска (base-dn)"

#: passwordchecker/passwordchecker-ui.c:674
msgid ""
"Specifies the search root for the desired record (e.g. 'dc=domain,dc=test,"
"dc=ru')"
msgstr ""
"Задает корень поиска для нужной записи.\n"
"Например: dc=domain,dc=test,dc=ru"

#: passwordchecker/passwordchecker-ui.c:680
msgid "Apply connection settings"
msgstr "Изменить параметры соединения"

#, c-format
msgid "Your password expires on %s"
msgstr "Пароль истекает %s"

msgid "Change password"
msgstr "Изменить пароль"

msgid "Change the notification settings"
msgstr "Изменить настройки уведомлений"

msgid "Password change required"
msgstr "Требуется смена пароля"

msgid "Automatic LDAP url calculation failed"
msgstr "Не удалось автоматически определить адрес LDAP сервера (url)"

msgid "You could specify the data manually in PasswordCheckerSettings"
msgstr "Вы можете вручную определить запись в PasswordCheckerSettings"

msgid "Automatic search root calculation failed"
msgstr "Не удалось автоматически определить корень поиска (base-dn)"

msgid "PasswordChecker: change settings"
msgstr "PasswordChecker: изменение настроек"

msgid "days"
msgstr "дней"

msgid "hours"
msgstr "часов"

msgid "min"
msgstr "минут"

msgid "Enable notifications at system startup"
msgstr "Включить уведомления при запуске системы"
